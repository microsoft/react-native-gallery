    "@react-native-picker/picker": "2.4.10",
    "@react-native/metro-config": "0.81.5",
    "@react-navigation/drawer": "7.7.7",
    "@react-navigation/native": "7.1.23",
    "@react-navigation/stack": "^6.3.12",
    "@rnx-kit/metro-config": "^2.1.2",
